class test {
	int var;
	int get_n() property { return var; }
	void set_n(int n) property { var = n; }
	test& set_n(int n) {
		var = n;
	}
}
void main() {
	test t;
	t.n = 12;
	alert("test", t.set_n(20).n);
}
