void main() {
	float f1 = 10;
	float f2 = 20;
	swapf(f2, f1);
	alert("test", f1 + " " + f2);
}


void swapf(float& f1, float& f2) {
	float tmp = f2;
	f2 = f1;
	f1 = tmp;
}
